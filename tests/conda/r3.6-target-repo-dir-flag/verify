#!/bin/sh

jupyter server extension list 2>&1 | grep jupyter_server_proxy
jupyter nbclassic-nbextension list 2>&1 | grep jupyter_server_proxy

R -e "library('digest')"

# Fail if R version is not 3.6
R --version
R -e 'if (!(version$major == "3" && as.double(version$minor) >= 6 && as.double(version$minor) < 7)) quit("yes", 1)'

./verify.py
